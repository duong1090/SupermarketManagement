@model IEnumerable<SupermarketManagement.Models.Receipt_Detail>
@{
    ViewData["Title"] = "IndexReceiptDetail";
}

<br />
<br />

<div>

    <table class="table table-striped border">
        <tr class="table-info">
            <th>
                @Html.DisplayNameFor(m => m.ProductID)
            </th>
            <th>
                @Html.DisplayNameFor(m => m.Product.Name)
            </th>
            <th>
                @Html.DisplayNameFor(m => m.Product.Price)
            </th>
            <th>
                @Html.DisplayNameFor(m => m.Amount)
            </th>
            <th>
                @Html.DisplayNameFor(m => m.ExpiryDate)
            </th>
            <th>
                @Html.DisplayNameFor(m => m.ManufactureDate)
            </th>
        </tr>

        @foreach (var item in Model)
        {
    <tr>
        <td>
            @Html.DisplayFor(m => item.ProductID)
        </td>
        <td>
            @Html.DisplayFor(m => item.Product.Name)
        </td>
        <td>
            @Html.DisplayFor(m => item.Product.Price)
        </td>
        <td>
            @Html.DisplayFor(m => item.Amount)
        </td>
        <td>
            @Html.DisplayFor(m => item.ExpiryDate)
        </td>
        <td>
            @Html.DisplayFor(m => item.ManufactureDate)
        </td>
    </tr>
        }
    </table>
    <div class="form-group">
        <a asp-action="Index" class="btn btn-success">Back to List</a>
    </div>

</div>


